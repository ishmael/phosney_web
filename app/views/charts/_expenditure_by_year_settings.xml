<settings>
  <preloader_on_reload>1</preloader_on_reload>
  <add_time_stamp>1</add_time_stamp>
  <background>
    <alpha>100</alpha>
    <border_alpha>20</border_alpha>
  </background>
  <plot_area>
    <margins>
      <right>40</right>
    </margins>
  </plot_area>
  <grid>
    <category>
      <dashed>1</dashed>
    </category>
    <value>
      <dashed>1</dashed>
    </value>
  </grid>
  <axes>
    <category>
      <width>1</width>
      <color>E7E7E7</color>
    </category>
    <value>
      <width>1</width>
      <color>E7E7E7</color>
    </value>
  </axes>
  <values>
    <value>
      <min>0</min>
    </value>
  </values>
  <column>
    <type>stacked</type>
    <width>85</width>
    <balloon_text>{description}</balloon_text>
    <grow_time>1</grow_time>
  </column>
  <depth>15</depth>
  <legend>
  	<enabled>true</enabled>
  </legend>

</settings>