        <div class="tabbertab">
            <h2><%=  I18n.t('layout.loanaccounts.title') %></h2>
            <div class="container1">
<% form_for(@loanaccount) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label I18n.t('layout.loanaccounts.accountname') %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label I18n.t('layout.loanaccounts.accountnumber') %><br />
    <%= f.text_field :number %>
  </p>
  <p>
    <%= f.label I18n.t('layout.loanaccounts.accountbank') %><br />
    <%= f.text_field :bank %>
  </p>
  <p>
    <%= f.submit I18n.t('layout.application.update')  %>
  </p>
<% end %>

<%= link_to I18n.t('layout.application.back'), loanaccounts_path %>
            </div><!--/end .container1-->
        </div><!--/end .tabbertab-->